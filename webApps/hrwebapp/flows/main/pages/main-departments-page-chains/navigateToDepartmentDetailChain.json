{
  "root": "navigateToDetail",
  "variables": {
    "departmentId": {
      "type": "string",
      "input": "fromCaller",
      "required": true
    }
  },
  "actions": {
    "navigateToDetail": {
      "module": "vb/action/builtin/navigateAction",
      "parameters": {
        "page": "main-department-detail",
        "params": {
          "departmentId": "[[ $variables.departmentId ]]"
        }
      }
    }
  }
}